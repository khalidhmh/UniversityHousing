  "homepage": "./",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "vite preview",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron\"",
    "electron-build": "cross-env NODE_ENV=production npm run build && electron-builder",
    "build:exe": "cross-env NODE_ENV=production npm run build && electron-builder --win --publish never",
    "build:exe-portable": "cross-env NODE_ENV=production npm run build && electron-builder --win -p never --config electron-builder.yml",
    "test": "vitest",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:reset-clean": "node scripts/reset-database.js"
  },
